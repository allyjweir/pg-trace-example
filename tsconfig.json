{
    "compilerOptions": {
        "lib": [
            "dom",
            "es5",
            "es6"
        ],
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "outDir": "./dist",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "alwaysStrict": true,
        "strictNullChecks": true,
        "sourceMap": true
    },
    "exclude": [
        "node_modules",
        "dist"
    ]
}
